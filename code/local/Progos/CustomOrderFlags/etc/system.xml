                            <show_in_store>0</show_in_store>
                        </orderstatus>
                        <status translate="label">
                            <label>Add Dhl Status For Tracking</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <comment>Add comma (,) separate status. 0 => In processing , 1 => Delivered , 3 => Shippment Not available</comment>
                        </status>
                    </fields>
                </dhlstatus>
            </groups>
        </dhllabel>
        <aramexsettings translate="label" module="shipping">
            <groups>
                <aramexlabelstatus translate="label">
                    <label>Aramex Status Cron</label>
