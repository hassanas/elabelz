<?xml version="1.0"?>
<config>
    <sections>
        <mageworx_ordersmanagement translate="label" module="mageworx_ordersgrid">
            <groups>
                <customorderflag translate="label">
                    <label>Custom Order Flag</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>5500</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label">
                            <label>Enable</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <comment>If it yes then Mageworx order grid will not add any additional join with core order table for Custom flags</comment>
                        </enabled>
                        <oos translate="label">
                            <label>Oos Flag Sync</label>
                            <frontend_type>button</frontend_type>
                            <frontend_model>Progos_CustomOrderFlags_Block_Adminhtml_System_Config_Form_Oos</frontend_model>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </oos>
                        <customer translate="label">
                            <label>Customer Flag Sync</label>
                            <frontend_type>button</frontend_type>
                            <frontend_model>Progos_CustomOrderFlags_Block_Adminhtml_System_Config_Form_Customer</frontend_model>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </customer>
                        <preffered translate="label">
                            <label>Preffered Courier Flag Sync</label>
                            <frontend_type>button</frontend_type>
                            <frontend_model>Progos_CustomOrderFlags_Block_Adminhtml_System_Config_Form_Preffered</frontend_model>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </preffered>
                    </fields>
                </customorderflag>
            </groups>
        </mageworx_ordersmanagement>
        <upslabel translate="label" module="upslabel">
            <groups>
                <upsstatus translate="label">
                    <label>Ups Status Cron</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>6001</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label">
                            <label>Enable</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>1</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                        </enabled>
                        <no_of_orders_process translate="label">
                            <label>Number of orders to process</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>2</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <comment>(Default value 20)</comment>
                        </no_of_orders_process>
                        <orderstatus translate="label">
                            <label>Update Order Status</label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>3</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                        </orderstatus>
                        <status translate="label">
                            <label>Add Ups Status For Tracking</label>
                            <frontend_type>text</frontend_type>
                            <sort_order>4</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>0</show_in_website>
                            <show_in_store>0</show_in_store>
                            <comment>Add comma (,) separate status. 0 => In processing , 1 => Delivered , 3 => Shippment Not available</comment>
                        </status>
                    </fields>
                </upsstatus>
            </groups>
        </upslabel>
        <dhllabel translate="label" module="dhllabel">
